program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {spmenu},
  MainMenu in 'MainMenu.pas' {FormUtama};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormUtama, FormUtama);
  Application.CreateForm(Tspmenu, Tspmenu);
  Application.Run;
end.
